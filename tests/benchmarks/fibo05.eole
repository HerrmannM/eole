id    = i->i.

succ  = n->x->y->(x ((n x) y)).

add   = m->n->x->y->(m x (n x y)).

pair  = x->y->p->((p x) y).

fst   = x->y->x.

snd   = x->y->y.

zero  = f->x->x.

one   = succ zero.

nextfibo =
  p ->
    with n1 = (p fst),
    with n2 = (p snd),
    (pair (add n1 n2) n1).

fibo  = n->(n nextfibo (pair zero one) fst).

(fibo (f->x->(f(f(f(f(f(x)))))))) id id.
